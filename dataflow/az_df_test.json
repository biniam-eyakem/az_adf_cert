{
	"name": "az_df_test",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "az_ds_raw_order_items",
						"type": "DatasetReference"
					},
					"name": "OrderItemsRaw"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_test_sink_order_items",
						"type": "DatasetReference"
					},
					"name": "OrderItemsTestSink"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          ORDER_ID as short,",
				"          LINE_ITEM_ID as short,",
				"          PRODUCT_ID as short,",
				"          UNIT_PRICE as double,",
				"          QUANTITY as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> OrderItemsRaw",
				"OrderItemsRaw sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('roundRobin', 4)) ~> OrderItemsTestSink"
			]
		}
	}
}